<mvc:View
    controllerName="changecolor.controller.View1"
    xmlns:blockcolor="sap.uxap.sample.SharedBlocks"
    xmlns="sap.uxap"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    
>
	<Table id="samplesTable" headerText="Color Palette in a Popover" class="sapUiLargeMarginBottom">
		<columns>
			<Column id="_IDGenColumn1">
				<Text id="_IDGenText1" text="Description"/>
			</Column>
			<Column id="_IDGenColumn2" width="30%">
				<Text id="_IDGenText2" text="Action"/>
			</Column>
		</columns>

		<ColumnListItem id="_IDGenColumnListItem1">
			<Label id="_IDGenLabel1" text='Default set of colors with both "Default Color" and "More Colors..." buttons'/>
			<Button id="_IDGenButton1" press="openFullSample" icon="sap-icon://text-color"/>
		</ColumnListItem>

	</Table>
    <Text id="_IDGenText3" text="Prueba cambio de color"></Text>
